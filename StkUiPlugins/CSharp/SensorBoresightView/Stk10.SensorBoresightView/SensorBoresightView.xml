<?xml version = "1.0"?>

<AGIRegistry version="1.0">
	<CategoryRegistry>
		<Category Name="UiPlugins">

			<!-- A standard context menu extension -->
 			
	 		<UiPlugin ProgID="STK10.AGI.SensorBoresightViewPlugin"
	 		File="AGI.SensorBoresightViewPlugin.dll" 
	 		DisplayName="Sensor Boresight View Ui Plugin" 
	 		AlwaysEnabled="true">

			<!-- 
				AGI licenses required by the extension 
			-->
				<LicenseSet Strict="false" Group="STKProfessional,STKExpert"/>

			<!-- 
				VersionInfo element contains information about the extension 
			-->
				<VersionInfo Version="1.1">
			               <Publisher>Analytical Graphics, Inc.</Publisher> 
			               <SupportUrl>http://www.agi.com/</SupportUrl> 
			               <Creator>Analytical Graphics, Inc.</Creator> 
			               <Description>The Ui Plugin exposes a context menu item for sensors that allows setting the 3D display to the FOV of the sensor</Description> 
				</VersionInfo>
			</UiPlugin>
		</Category>
	</CategoryRegistry>
</AGIRegistry>
